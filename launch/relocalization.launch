<launch>
  <node pkg="rosbag" type="play" name="player" output="screen" args="$(find mapping_pfoe)/bag_and_maps/20190617_square.bag" required="true">
    <remap from="/map" to="/notuse"/>
  </node>
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find mapping_pfoe)/config/relocalization.rviz"/>
  <arg name="map_file" default="$(find mapping_pfoe)/bag_and_maps/20190617_square.yaml"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />
</launch>
